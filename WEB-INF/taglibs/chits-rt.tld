 <tag>
   <description>
       Displays the height observation value as centimeters and feet and inches 
   </description>
   <name>height</name>
   <tag-class>org.openmrs.module.chits.web.taglib.HeightTag</tag-class>
   <body-content>empty</body-content>
   <attribute>
       <description>The Observation representing a value in centimeters (double value)</description>
       <name>obs</name>
       <required>true</required>
       <rtexprvalue>true</rtexprvalue>
       <type>org.openmrs.Obs</type>
   </attribute>
   <attribute>
       <description>Whether to show the date the observation was taken on</description>
       <name>showDateTaken</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
       <type>java.lang.Boolean</type>
   </attribute>
   <attribute>
       <description>Whether to show the elapsed amount of time since this observation was taken</description>
       <name>showElapsedSinceTaken</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
       <type>java.lang.Boolean</type>
   </attribute>
   <attribute>
       <description>Text to display if no data is available.</description>
       <name>noEnteredDataText</name>
       <required>false</required>
       <rtexprvalue>true</rtexprvalue>
       <type>java.lang.String</type>
   </attribute>
 </tag>

