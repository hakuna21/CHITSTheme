 <!-- CHITS Functions -->
 
 <function>
   <description>
     Returns an ordered List of observation matching the given cached concept id sorted by ascending observation date / time.
     &lt;ul&gt;
	 &lt;li&gt;If the 'var' parameter is an Encounter, the encounter's observations are searched.&lt;/li&gt;
	 &lt;li&gt;If the 'var' parameter is an Obs, the observation group's members are searched.&lt;/li&gt;
	 &lt;li&gt;If the 'var' parameter is a Person, all matching observations for that person are returned.&lt;/li&gt;
     &lt;/ul&gt;
   </description>
   <name>observations</name>
   <function-class>org.openmrs.module.chits.web.taglib.Functions</function-class>
   <function-signature>java.util.List observations(java.lang.Object, java.lang.Object)</function-signature>
   <example>
     &lt;c:if test="${not empty chits:observations(${patientQueue.encounter}, VisitConcepts.VITAL_SIGNS)}" &gt;
   </example>
 </function>

 <function>
   <description>
     Filters a List of observations to include only those with the specified coded value.
   </description>
   <name>filterByCodedValue</name>
   <function-class>org.openmrs.module.chits.web.taglib.Functions</function-class>
   <function-signature>java.util.List filterByCodedValue(java.util.List, org.openmrs.module.chits.CachedConceptId)</function-signature>
   <example>
     &lt;c:if test="${not empty chits:filterByCodedValue(chits:observations(patientQueue.encounter, MCServiceRecordConcepts.SERVICE_TYPE), MCServiceTypes.VITAMIN_A_SUPPLEMENTATION)}" &gt;
   </example>
 </function>
  
 <function>
   <description>
     Returns a reversed version of the given list.
   </description>
   <name>reverse</name>
   <function-class>org.openmrs.module.chits.web.taglib.Functions</function-class>
   <function-signature>java.util.List reverse(java.util.List)</function-signature>
   <example>
     &lt;c:if test="${not empty chits:reverse(chits:observations(patientQueue.encounter, MCServiceRecordConcepts.SERVICE_TYPE))}" &gt;
   </example>
 </function>
  
 <function>
   <description>
     Returns the first observation matching the given the cached concept id, or null if the observation was not found.
     &lt;ul&gt;
	 &lt;li&gt;If the 'var' parameter is an Encounter, the encounter's first matching observation is returned.&lt;/li&gt;
	 &lt;li&gt;If the 'var' parameter is an Obs, the observations group member's first matching observation is returned.&lt;/li&gt;
	 &lt;li&gt;If the 'var' parameter is a Person, the first matching observation for that person is returned.&lt;/li&gt;
     &lt;/ul&gt;
   </description>
   <name>observation</name>
   <function-class>org.openmrs.module.chits.web.taglib.Functions</function-class>
   <function-signature>org.openmrs.Obs observation(java.lang.Object, java.lang.Object)</function-signature>
   <example>
     &lt;c:out value="${chits:observation(${patientQueue.encounter}, VisitConcepts.WEIGHT_KG).valueText}" /&gt;
   </example>
 </function>
 
 <function>
   <description>
     Returns the Concept instance matching the given cached concept id, or null if the Concept was not found.
   </description>
   <name>concept</name>
   <function-class>org.openmrs.module.chits.web.taglib.Functions</function-class>
   <function-signature>org.openmrs.Concept concept(org.openmrs.module.chits.CachedConceptId)</function-signature>
   <example>
     &lt;c:set var="weightConcept" value="${chits:concept(VisitConcepts.WEIGHT_KG)}" /&gt;
   </example>
 </function>
 
 <function>
   <description>
     Returns the Concept instance matching the given concept id or name, or null if the Concept was not found.
   </description>
   <name>conceptByIdOrName</name>
   <function-class>org.openmrs.module.chits.web.taglib.Functions</function-class>
   <function-signature>org.openmrs.Concept conceptByIdOrName(java.lang.String)</function-signature>
   <example>
     &lt;c:set var="concept" value="${chits:conceptByIdOrName('6272')}" /&gt;
   </example>
 </function>

 <function>
   <description>
     Return the corresponding mapping source code value for the given concept and source name
   </description>
   <name>mapping</name>
   <function-class>org.openmrs.module.chits.web.taglib.Functions</function-class>
   <function-signature>java.lang.String mapping(org.openmrs.Concept, java.lang.String)</function-signature>
   <example>
     &lt;c:set var="icd10Code" value="${chits:concept(concept, 'ICD10')}" /&gt;
   </example>
 </function>

 <function>
